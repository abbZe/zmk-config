#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/pointing.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        layer_0 {
            bindings = <
&kp Q  &kp W  &kp E  &kp R                 &kp T                        &kp Y                &kp U        &kp I      &kp O    &kp P
&kp A  &kp S  &kp D  &kp F                 &kp G                        &kp H                &kp J        &kp K      &kp L    &kp SEMI
&kp Z  &kp X  &kp C  &kp V                 &kp B                        &kp N                &kp M        &kp COMMA  &kp DOT  &kp SQT
                     &mt LEFT_SHIFT SPACE  &mt LEFT_ALT LEFT_BRACKET    &lt 1 RIGHT_BRACKET  &lt 2 ENTER
            >;
        };

        layer_1 {
            bindings = <
&kp N1            &kp N2       &kp N3  &kp N4            &kp N5            &kp N6          &kp N7          &kp N8        &kp N9           &kp N0
&kp LS(ESCAPE)    &kp LS(TAB)  &trans  &kp LS(LPAR)      &kp LS(LBRC)      &mmv MOVE_LEFT  &mmv MOVE_DOWN  &mmv MOVE_UP  &mmv MOVE_RIGHT  &kp BSPC
&kp LEFT_COMMAND  &trans       &kp C   &kp V             &kp LBKT          &kp RBKT        &kp N0          &kp LS(DLLR)  &trans           &trans
                                       &kp LEFT_CONTROL  &kp LEFT_SHIFT    &trans          &trans
            >;
        };

        layer_2 {
            bindings = <
&trans  &trans  &trans  &trans  &trans            &trans  &trans  &trans     &trans     &kp SQT
&trans  &trans  &trans  &trans  &trans            &trans  &trans  &kp EQUAL  &kp MINUS  &kp SEMI
&trans  &trans  &trans  &trans  &trans            &trans  &trans  &trans     &kp SLASH  &kp BACKSLASH
                        &trans  &kp LEFT_SHIFT    &trans  &trans
            >;
        };
    };
};
